{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [
      0
    ],
    "react/jsx-filename-extension": [
      0
    ]
  }
}
